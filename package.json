{
  "name": "@kolhe/eslint-config",
  "version": "2.2.0",
  "packageManager": "pnpm@9.6.0",
  "description": "One eslint config to rule them all",
  "type": "module",
  "license": "MIT",
  "homepage": "https://github.com/sumitkolhe/eslint-config#readme",
  "bugs": {
    "url": "https://github.com/sumitkolhe/eslint-config/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sumitkolhe/eslint-config.git"
  },
  "files": [
    "dist"
  ],
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": "./dist/index.js",
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "dev": "tsup --format esm,cjs --watch & eslint-flat-config-viewer",
    "build": "tsup",
    "lint": "eslint .",
    "lint:fix": "pnpm run lint --fix",
    "release": "bumpp && pnpm publish",
    "prepublishOnly": "pnpm run build"
  },
  "peerDependencies": {
    "eslint": "^8.56.0 || ^9.0.0"
  },
  "dependencies": {
    "@unocss/eslint-plugin": "^0.59.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-define-config": "^1.24.1",
    "eslint-plugin-antfu": "^2.1.2",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-import-x": "^0.5.0",
    "eslint-plugin-jsonc": "^2.15.0",
    "eslint-plugin-markdown": "^4.0.1",
    "eslint-plugin-n": "^16.6.2",
    "eslint-plugin-perfectionist": "^2.8.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-unicorn": "^52.0.0",
    "eslint-plugin-unused-imports": "^3.1.0",
    "eslint-plugin-vue": "^9.24.0",
    "eslint-plugin-yml": "^1.14.0",
    "globals": "^15.0.0",
    "jsonc-eslint-parser": "^2.4.0",
    "local-pkg": "^0.5.0",
    "prettier": "^3.2.5",
    "typescript-eslint": "^7.5.0",
    "vue-eslint-parser": "^9.4.2",
    "yaml-eslint-parser": "^1.2.2"
  },
  "devDependencies": {
    "@types/node": "^20.12.5",
    "bumpp": "^9.4.0",
    "eslint": "^9.0.0",
    "eslint-flat-config-viewer": "^0.1.11",
    "sucrase": "^3.35.0",
    "tsup": "^8.0.2",
    "typescript": "^5.4.4"
  },
  "engines": {
    "node": "^18.18.0 || >=20.0.0"
  }
}
