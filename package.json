{
  "name": "@kolhe/eslint-config",
  "version": "2.2.0",
  "packageManager": "pnpm@9.6.0",
  "description": "One eslint config to rule them all",
  "type": "module",
  "license": "MIT",
  "homepage": "https://github.com/sumitkolhe/eslint-config#readme",
  "bugs": {
    "url": "https://github.com/sumitkolhe/eslint-config/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sumitkolhe/eslint-config.git"
  },
  "files": [
    "dist"
  ],
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": "./dist/index.js",
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "dev": "tsup --format esm,cjs --watch & eslint-flat-config-viewer",
    "build": "tsup",
    "lint": "eslint .",
    "lint:fix": "pnpm run lint --fix",
    "release": "bumpp && pnpm publish",
    "prepublishOnly": "pnpm run build"
  },
  "peerDependencies": {
    "eslint": "^8.56.0 || ^9.0.0"
  },
  "dependencies": {
    "@unocss/eslint-plugin": "^0.61.5",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-antfu": "^2.3.4",
    "eslint-plugin-command": "^0.2.3",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-import-x": "^3.0.1",
    "eslint-plugin-jsdoc": "^48.8.3",
    "eslint-plugin-jsonc": "^2.16.0",
    "eslint-plugin-markdown": "^5.1.0",
    "eslint-plugin-n": "^17.9.0",
    "eslint-plugin-perfectionist": "^3.0.0",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-unicorn": "^54.0.0",
    "eslint-plugin-unused-imports": "^4.0.1",
    "eslint-plugin-vue": "^9.27.0",
    "eslint-plugin-yml": "^1.14.0",
    "globals": "^15.8.0",
    "jsonc-eslint-parser": "^2.4.0",
    "local-pkg": "^0.5.0",
    "prettier": "^3.3.3",
    "typescript-eslint": "^7.17.0",
    "vue-eslint-parser": "^9.4.3",
    "yaml-eslint-parser": "^1.2.3"
  },
  "devDependencies": {
    "@types/eslint": "^9.6.0",
    "eslint-flat-config-viewer": "^0.1.11",
    "@types/node": "^20.14.11",
    "bumpp": "^9.4.1",
    "eslint": "npm:eslint-ts-patch@9.5.0-0",
    "eslint-ts-patch": "9.5.0-0",
    "tsup": "^8.2.2",
    "tsx": "^4.16.2",
    "typescript": "^5.5.3"
  },
  "engines": {
    "node": "^18.18.0 || >=20.0.0"
  },
  "pnpm": {
    "overrides": {
      "is-core-module": "npm:@no-shims/is-core-module"
    }
  }
}
