{
  "name": "@kolhe/eslint-config",
  "version": "2.0.1",
  "packageManager": "pnpm@8.15.3",
  "description": "One eslint config to rule them all",
  "type": "module",
  "license": "MIT",
  "homepage": "https://github.com/sumitkolhe/eslint-config#readme",
  "bugs": {
    "url": "https://github.com/sumitkolhe/eslint-config/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sumitkolhe/eslint-config.git"
  },
  "files": [
    "dist"
  ],
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    "require": "./dist/index.cjs",
    "import": "./dist/index.js"
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "dev": "tsup --format esm,cjs --watch & eslint-flat-config-viewer",
    "build": "tsup",
    "lint": "eslint .",
    "lint:fix": "pnpm run lint --fix",
    "release": "bumpp && pnpm publish",
    "prepublishOnly": "pnpm run build"
  },
  "peerDependencies": {
    "eslint": "^8.56.0 || ^9.0.0"
  },
  "dependencies": {
    "@unocss/eslint-plugin": "^0.58.6",
    "eslint-config-prettier": "^9.1.0",
    "eslint-define-config": "^2.1.0",
    "eslint-plugin-antfu": "^2.1.2",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-i": "^2.29.1",
    "eslint-plugin-jsonc": "^2.13.0",
    "eslint-plugin-markdown": "^4.0.1",
    "eslint-plugin-n": "^16.6.2",
    "eslint-plugin-perfectionist": "^2.7.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-unicorn": "^51.0.1",
    "eslint-plugin-unused-imports": "^3.1.0",
    "eslint-plugin-vue": "^9.23.0",
    "eslint-plugin-yml": "^1.12.2",
    "globals": "^14.0.0",
    "jsonc-eslint-parser": "^2.4.0",
    "local-pkg": "^0.5.0",
    "prettier": "^3.2.5",
    "typescript-eslint": "^7.2.0",
    "vue-eslint-parser": "^9.4.2",
    "yaml-eslint-parser": "^1.2.2"
  },
  "devDependencies": {
    "@types/node": "^20.11.27",
    "bumpp": "^9.4.0",
    "eslint": "^8.57.0",
    "eslint-flat-config-viewer": "^0.1.11",
    "tsup": "^8.0.2",
    "typescript": "^5.4.2"
  },
  "engines": {
    "node": "^18.18.0 || >=20.0.0"
  }
}
